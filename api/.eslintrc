{
  "plugins": [],
  "env": {
    "node": true,
    "es6": true
  },
  "extends": "airbnb/base",
  "rules": {
    "no-console": "off",
    "object-shorthand": "off",
    "keyword-spacing": "off",
    "strict": "off",
    "max-len": "off",
    "no-param-reassign": "off",
    "arrow-body-style": "off",
    "no-restricted-syntax": "off",
    "global-require": "off",
    "no-underscore-dangle": "off",
    "import/no-extraneous-dependencies": "off",
    "import/no-unresolved": "off",
    "guard-for-in": "off"
  },
  "globals" : {
  }
}